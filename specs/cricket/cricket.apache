Alias /cricket/images   @imgdir@
Alias /cricket          @cgidir@

<Directory @cgidir@>
    Options +ExecCGI
    Order Deny,Allow
    Deny from all
    Allow from localhost
</Directory>

<Directory @imgdir@>
    Order Deny,Allow
    Deny from all
    Allow from localhost
</Directory>

# vim:ft=apache
